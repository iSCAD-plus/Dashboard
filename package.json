  "scripts": {
    "dev": "kyt dev",
    "build": "kyt build",
    "start": "node build/server/main.js",
    "proto": "kyt proto",
    "test": "yarn jest -- --forceExit",
    "test:watch": "yarn jest -- --watch",
    "lint": "./node_modules/.bin/eslint ./src --ext .js",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "lint:errors": "yarn lint -- --quiet",
    "lint:nibble": "./node_modules/.bin/eslint-nibble src/ mongo/",
    "lint:fix": "yarn lint -- --fix",
    "jest": "./node_modules/.bin/jest",
    "prepush": "yarn jest -- --forceExit",
    "precommit": "yarn lint:staged",
    "format": "./node_modules/.bin/prettier-eslint \"*(src|mongo)/**/*.js\" --write",
    "kyt:help": "kyt --help"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add",
      "./node_modules/.bin/eslint"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
