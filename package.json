    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-router": "2.6.1"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
